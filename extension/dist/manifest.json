{
    "version": "4.2.0",
    "manifest_version": 3,
    "name": "ReviewRune",
    "description": "This is a description",
    "content_scripts": [
        {
            "matches": [
                "https://*.google.com/*"
            ],
            "js": [
                "./js/content.js"
            ]
        }
    ],
    "action": {
        "default_popup": "js/index.html",
        "default_title": "ReviewRune"
    }
}